    schema:genre "cs.AI"^^schema:Text,
        "cs.CL"^^schema:Text,
        "cs.NE"^^schema:Text ;
    schema:headline "LSTMVis: A Tool for Visual Analysis of Hidden State Dynamics in  Recurrent Neural Networks"^^schema:Text ;
