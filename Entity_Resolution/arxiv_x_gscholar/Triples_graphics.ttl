    schema:genre "cs.CY"^^schema:Text,
        "cs.SI"^^schema:Text ;
    schema:headline "Situational Awareness Enhanced through Social Media Analytics: A Survey  of First Responders"^^schema:Text ;
    schema:url "http://arxiv.org/abs/1909.07316v1"^^schema:URL .

