        "Ruoming Pang"^^schema:Person,
        "Vijay Vasudevan"^^schema:Person,
        "Weijun Wang"^^schema:Person,
        "Yukun Zhu"^^schema:Person ;
    schema:commentCount "214"^^schema:Integer ;
    schema:dateModified "2019-11-20T17:26:40Z"^^schema:DateTime ;
    schema:datePublished "2019-05-06T19:38:31Z"^^schema:DateTime ;
    schema:genre "cs.CV"^^schema:Text ;
    schema:headline "Searching for MobileNetV3"^^schema:Text ;
