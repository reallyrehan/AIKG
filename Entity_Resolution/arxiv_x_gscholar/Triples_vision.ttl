    schema:datePublished "2018-01-29T09:23:55Z"^^schema:DateTime ;
    schema:genre "cs.CV"^^schema:Text ;
    schema:headline "CosFace: Large Margin Cosine Loss for Deep Face Recognition"^^schema:Text ;
    schema:publisher "Proceedings of the IEEE Conference on Computer Vision and Pattern …"^^schema:Periodical ;
