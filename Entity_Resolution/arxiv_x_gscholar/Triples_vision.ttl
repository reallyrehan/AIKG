    schema:datePublished "2016-11-20T23:39:52Z"^^schema:DateTime ;
    schema:genre "cs.CV"^^schema:Text ;
    schema:headline "RefineNet: Multi-Path Refinement Networks for High-Resolution Semantic  Segmentation"^^schema:Text ;
    schema:publisher "Proceedings of the IEEE Conference on Computer Vision and Pattern …"^^schema:Periodical ;
