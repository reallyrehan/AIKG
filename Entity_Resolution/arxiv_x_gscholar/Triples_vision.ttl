    schema:datePublished "2018-04-20T17:00:14Z"^^schema:DateTime ;
    schema:genre "cs.CV"^^schema:Text ;
    schema:headline "Image Inpainting for Irregular Holes Using Partial Convolutions"^^schema:Text ;
    schema:publisher "Proceedings of the European Conference on Computer Vision (ECCV), 85-100"^^schema:Periodical ;
    schema:url "http://arxiv.org/abs/1804.07723v2"^^schema:URL,
