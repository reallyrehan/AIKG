    schema:genre "cs.AI"^^schema:Text,
        "cs.CV"^^schema:Text,
        "cs.MM"^^schema:Text ;
    schema:headline "CREST: Convolutional Residual Learning for Visual Tracking"^^schema:Text ;
    schema:publisher "Proceedings of the IEEE International Conference on Computer Vision, 2555-2564"^^schema:Periodical ;
