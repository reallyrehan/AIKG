        "Vladimir Golkov"^^schema:Person ;
    schema:commentCount "1440"^^schema:Integer ;
    schema:dateModified "2015-05-04T08:50:57Z"^^schema:DateTime ;
    schema:datePublished "2015-04-26T17:30:32Z"^^schema:DateTime ;
    schema:genre "I.2.6; I.4.8"^^schema:Text,
