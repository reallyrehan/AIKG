    schema:genre "cs.CV"^^schema:Text,
        "cs.LG"^^schema:Text,
        "cs.NE"^^schema:Text ;
    schema:headline "Learning to Generate Chairs, Tables and Cars with Convolutional Networks"^^schema:Text ;
    schema:publisher "IEEE Transactions on Pattern Analysis and Machine Intelligence 39 (4), 692-705"^^schema:Periodical ;
    schema:url "http://arxiv.org/abs/1411.5928v4"^^schema:URL,
