    schema:datePublished "2017-12-15T23:56:52Z"^^schema:DateTime ;
    schema:genre "cs.LG"^^schema:Text,
        "stat.ML"^^schema:Text ;
    schema:headline "Quantization and Training of Neural Networks for Efficient  Integer-Arithmetic-Only Inference"^^schema:Text ;
