        "cs.CV"^^schema:Text,
        "cs.IR"^^schema:Text ;
    schema:headline "What's Cookin'? Interpreting Cooking Videos using Text, Speech and  Vision"^^schema:Text ;
    schema:publisher "HLT-NAACL, 143-152"^^schema:Periodical ;
    schema:url "http://arxiv.org/abs/1503.01558v3"^^schema:URL,
