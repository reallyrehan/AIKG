    schema:datePublished "2016-03-19T00:22:20Z"^^schema:DateTime ;
    schema:genre "cs.CL"^^schema:Text ;
    schema:headline "A Fast Unified Model for Parsing and Sentence Understanding"^^schema:Text ;
    schema:publisher "Proceedings of the 54th Annual Meeting of the Association for Computational …"^^schema:Periodical ;
    schema:url "http://arxiv.org/abs/1603.06021v3"^^schema:URL,
