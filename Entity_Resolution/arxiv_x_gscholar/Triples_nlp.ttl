        "cs.CL"^^schema:Text,
        "cs.LG"^^schema:Text,
        "cs.NE"^^schema:Text ;
    schema:headline "Generating Factoid Questions With Recurrent Neural Networks: The 30M  Factoid Question-Answer Corpus"^^schema:Text ;
